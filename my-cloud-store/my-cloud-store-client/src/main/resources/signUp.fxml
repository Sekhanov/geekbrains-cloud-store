<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" minHeight="60.0" prefHeight="768.0" prefWidth="1024.0" fx:id="rootPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.skhanov.mycloudstoreclient.fxcontrollers.SignInFxController">
   <children>
      <HBox alignment="CENTER" prefHeight="250.0" prefWidth="1024.0" style="-fx-background-color: #2e3747;">
         <children>
            <Label prefHeight="100.0" prefWidth="528.0" text="My Cloud Store" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Bauhaus 93" size="75.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <TextField maxWidth="474.0" prefHeight="60.0" prefWidth="474.0" promptText="login" fx:id="loginTextField">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets top="100.0" />
         </VBox.margin>
      </TextField>
      <TextField maxHeight="60.0" maxWidth="474.0" prefHeight="60.0" prefWidth="474.0" promptText="password" fx:id="passwordTextField">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
      </TextField>
      <Button mnemonicParsing="false" onAction="#submit" prefHeight="70.0" prefWidth="146.0" text="SIGN IN" textFill="WHITE">
         <font>
            <Font name="Bodoni MT Bold" size="26.0" />
         </font>
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
